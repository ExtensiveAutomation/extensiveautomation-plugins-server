		ret = self.repogit.add(commitMsg="samples version 15.0.0")
		if not ret:
			self.error("unable to commit the repo")

		ret = self.repogit.push()
		if not ret:
			self.error("unable to push the repo")
			
		self.step1.setPassed(actual="success")
